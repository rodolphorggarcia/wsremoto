package repeticao;

import javax.swing.JOptionPane;

public class DesafioDo {

	public static void main(String[] args) {
	
		
		int jogador1 = Integer.parseInt(JOptionPane.showInputDialog("Jogador1 digite o primeiro numero"));
		int jogador2 = 0;
		
		do {
			jogador2 = Integer.parseInt(JOptionPane.showInputDialog("Jodagor2 descubra o numero digitado"));
			
		}while(jogador1==jogador2);
		
		
		
		
		
		
		
		
		

	}

}
