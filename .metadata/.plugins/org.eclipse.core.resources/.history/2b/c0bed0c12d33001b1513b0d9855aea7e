package variaveis;

public class TiposDados {

	public static void main(String[] args) {
		
		
		String nome = "Rodolpho";
		int idade = 29;
		double altura = 1.81;
		double peso = 96.0;
		
		System.out.println("Nome: "+ nome);
		System.out.println(idade);
		System.out.println(altura);
		System.out.println(peso);
		
		
	

	}

}
